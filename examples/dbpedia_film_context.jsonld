{
  "@context": {
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "foaf": "http://xmlns.com/foaf/0.1/", 
    "dcterms": "http://purl.org/dc/terms/",
    "dbpo": "http://dbpedia.org/ontology/",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "Film": "dbpo:Film",
    "Person": "foaf:Person",
    "id": "@id",
    "types": "@type",
    "titles": {
      "@id": "rdfs:label",
      "@type": "xsd:langString",
      "@container": "@language"
    },
    "subjects": {
      "@id": "dcterms:subject",
      "@type": "@id",
      "@container": "@set"
    },
    "actors": {
      "@id": "dbpo:starring",
      "@type": "@id",
      "@container": "@set"
    },
    "names": {
      "@id": "foaf:name",
      "@type": "xsd:langString",
      "@container": "@language"
    }
  }
}