{
  "@context": {
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "foaf": "http://xmlns.com/foaf/0.1/", 
    "dcterms": "http://purl.org/dc/terms/",
    "umbel": "http://umbel.org/umbel/rc/",
    "dbpo": "http://dbpedia.org/ontology/",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "Film": "dbpo:Film",
    "Person": "foaf:Person",
    "id": "@id",
    "types": "@type",
    "title_en": {
      "@id": "rdfs:label",
      "@language": "en"
    },
    "title_fr": {
      "@id": "rdfs:label",
      "@language": "fr"
    },
    "subjects": {
      "@id": "dcterms:subject",
      "@type": "@id",
      "container": "@set"
    },
    "actors": {
      "@id": "dbpo:starring",
      "@type": "@id",
      "container": "@set"
    },
    "name": {
      "@id": "foaf:name",
      "@type": "xsd:string"
    }
  }
}